{
  "metadata": [
    {
      "src": [
        {
          "src": "../Assets/Scripts",
          "files": [
            "**.cs"
          ]
        }
      ],
      "dest": "api",
      "globalNamespaceId": "Global",
      "filter": "filterconfig.yml"
    }
  ],
  "build": {
    "template": [
      "statictoc"
    ],
    "content": [
      {
        "src": "api",
        "files": [ "**.yml", "index.md" ],
        "dest": "api"
      },
      {
        "files": [
          "*.md",
          "toc.yml"
        ]
      }
    ],
    "dest": "../_site"
  }
}
